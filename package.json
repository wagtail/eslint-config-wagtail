{
  "name": "@wagtail/eslint-config-wagtail",
  "version": "0.6.0-rc1",
  "description": "Shareable ESLint config for Wagtail",
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "author": "wagtail",
  "license": "MIT",
  "homepage": "https://github.com/wagtail/eslint-config-wagtail#readme",
  "bugs": {
    "url": "https://github.com/wagtail/eslint-config-wagtail/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wagtail/eslint-config-wagtail.git"
  },
  "keywords": [
    "config",
    "eslint-config",
    "eslint",
    "lint",
    "styleguide",
    "wagtail"
  ],
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.54.0"
  },
  "peerDependencies": {
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.0",
    "eslint-import-resolver-typescript": "^4.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript-eslint": "^8.54.0"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,md,css,scss,yaml,yml,json}'",
    "lint:format": "prettier --check '**/*.{js,md,css,scss,yaml,yml,json}'",
    "lint:js": "eslint ./ --max-warnings 0",
    "lint:readme": "node write-rules.mjs --check",
    "lint": "npm run lint:format && npm run lint:js && npm run lint:readme",
    "preflight": "npm run format && npm run lint && npm run test",
    "print-config": "eslint --print-config eslint.config.js",
    "test": "node --test",
    "write-rules": "node write-rules.mjs"
  },
  "prettier": {
    "singleQuote": true,
    "quoteProps": "consistent"
  }
}
